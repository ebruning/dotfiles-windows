[merge]
	tool = kdiff3
[mergetool "kdiff3"]
	path = c:/Program Files/KDiff3/kdiff3.exe
[diff]
	tool = kdiff3
	guitool = winmerge
[difftool "kdiff3"]
	path = c:/Program Files/KDiff3/kdiff3.exe
[user]
	name = Ethan Bruning
	email = ethan.bruning@kofax.com
[difftool "winmerge"]
	path = c:/Program Files/winmerge/winmergeu.exe
	cmd = \"c:/Program Files/winmerge/winmergeu.exe\" \"$LOCAL\" \"$REMOTE\"
[color]
	branch = auto
	status = auto
	diff = auto
	interactive = auto
[color "branch"]
	current = yellow
	local = yellow
	remote = green
[color "diff"]
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
[core]
	excludesfile = ~/.gitignore
	autocrlf = true
[branch "master"]
	remote = origin
	merge = refs/heads/master
